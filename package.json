{
  "name": "svg-sprite-generator",
  "version": "0.8.0",
  "description": "App to create SVG symbols files by simply dropping all your icons here",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json server/index.ts",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "build": "npm run build:server && npm run build:next",
    "prebuild": "rimraf dist && rimraf .next",
    "build:server": "tsc -p tsconfig.server.json",
    "build:next": "npm run lint && next build",
    "export:static": "next build && next export -o docs/",
    "lint": "npx eslint . --ext .ts,.tsx",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "generate:sprite": "svg-icon-generate --folder=./assets/material-icons --output=./assets/test.svg"
  },
  "keywords": [],
  "author": "Jan Suwart",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "express": "^4.17.1",
    "material-ui-dropzone": "^3.3.1",
    "moment": "^2.27.0",
    "mui-datatables": "^3.1.4",
    "next": "^11.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "recoil": "^0.0.10",
    "svg-icon-sprite": "^1.1.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^9.4.4",
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-plugin-react": "^7.20.3",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "sass": "^1.26.9",
    "typescript": "^3.9.6"
  }
}
